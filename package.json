{
  "name": "template-microservice",
  "version": "0.0.1",
  "description": "",
  "author": "Nguyen Thai Hoc",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix --max-warnings=0",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run build && npm run typeorm migration:generate -- -n",
    "migration:create": "typeorm migration:create -n",
    "migration:run": "typeorm migration:run",
    "migration:revert": "typeorm migration:revert",
    "entity:create": "typeorm entity:create -n",
    "postinstall": "husky install",
    "seeder": "ts-node -r tsconfig-paths/register src/console.ts seeding-data",
    "remove-temp-merge-expire": "ts-node -r tsconfig-paths/register src/console.ts remove-temp-merge-expire",
    "check-temp-merge-about-to-succeed": "ts-node -r tsconfig-paths/register src/console.ts check-temp-merge-is-about-to-succeed",
    "console:dev": "ts-node -r tsconfig-paths/register src/console.ts",
    "console": "node dist/src/console.js"
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint",
      "npm run format",
      "git add ."
    ]
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.7.2",
    "@grpc/proto-loader": "^0.7.3",
    "@nestjs/common": "8.4.7",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "8.4.7",
    "@nestjs/microservices": "8.4.7",
    "@nestjs/mongoose": "9.2.0",
    "@nestjs/passport": "9.0.0",
    "@nestjs/platform-express": "8.4.7",
    "@nestjs/schedule": "2.1.0",
    "@types/lodash": "4.14.186",
    "amqp-connection-manager": "^4.1.7",
    "amqplib": "^0.10.3",
    "axios": "0.27.2",
    "bignumber.js": "9.1.0",
    "cache-manager": "4.1.0",
    "cache-manager-redis-store": "2.0.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "ioredis": "^5.2.3",
    "joi": "17.6.3",
    "lodash": "4.17.21",
    "log4js": "6.7.0",
    "moment": "2.29.4",
    "mongoose": "6.6.5",
    "morgan": "1.10.0",
    "nestjs-console": "8.0.0",
    "reflect-metadata": "0.1.13",
    "remove": "0.1.5",
    "rimraf": "3.0.2",
    "rxjs": "7.5.7",
    "swagger-ui-express": "4.5.0",
    "web3": "1.8.0",
    "webpack": "5.74.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@nestjs/cli": "8.2.8",
    "@nestjs/schematics": "8.0.11",
    "@nestjs/swagger": "5.2.1",
    "@types/cache-manager": "4.0.2",
    "@types/express": "4.17.14",
    "@types/jest": "27.4.1",
    "@types/multer": "1.4.7",
    "@types/node": "16.11.65",
    "@typescript-eslint/eslint-plugin": "5.40.0",
    "@typescript-eslint/parser": "5.40.0",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.1",
    "jest": "27.5.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "source-map-support": "0.5.21",
    "supertest": "6.3.0",
    "ts-jest": "27.1.5",
    "ts-loader": "9.4.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "3.14.1",
    "typescript": "4.8.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
